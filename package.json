  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "token": "node ./token-generator.cjs",
    "token:dev": "node ./token-generator.cjs -i <some ident> -m dev",
    "token:local": "node ./token-generator.cjs -i <some ident> -m lokalt",
    "start:dev": "npm run token:dev && npm run dev",
    "start:local": "npm run token:local && npm run dev",
    "test:e2e": "npx playwright test",
    "test:e2e:mock": "SERVER_URL=\"http://localhost:3001\" MOCK_BACKEND=true npx playwright test"
  },
